#include <iostream>

using namespace std;

int main()
{
    int num;
    cout<<"please emter the number\n";
    cin>>num;
    for(int i=1;i<=num;i++){
        
        if(num % i == 0 && i != 1){
            cout<<i;
            i=num+1;
            
            
        }
        
        
    }

    return 0;
}

