#include <iostream>

using namespace std;
void findDiviser(int num){
  for(int i=1; i<=num ;i++){
      
    if(num%i==0){
        cout<<i<<" ";
    }else
    continue;
    
    
  }
}
int main()
{
    int num;
    cout<<"please enter the number ";
    cin>>num;
    findDiviser(num);

    return 0;
}
